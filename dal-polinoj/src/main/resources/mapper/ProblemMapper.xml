<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oj.dalpolinoj.mapper.ProblemMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.oj.dalpolinoj.domin.Problem">
        <id column="id" property="id" />
        <result column="description" property="description" />
        <result column="hint" property="hint" />
        <result column="input" property="input" />
        <result column="memory" property="memory" />
        <result column="output" property="output" />
        <result column="source" property="source" />
        <result column="time" property="time" />
        <result column="title" property="title" />
        <result column="author" property="author" />
        <result column="source_id" property="sourceId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, description, hint, input, memory, output, source, time, title, author, source_id
    </sql>

</mapper>
